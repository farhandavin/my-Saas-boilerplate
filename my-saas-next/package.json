    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "pg": "^8.16.3",
    "tailwindcss": "^4",
    "ts-morph": "^27.0.2",
    "tsx": "^4.21.0",
    "typescript": "^5",
