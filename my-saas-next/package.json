    "posthog-js": "^1.312.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.69.0",
    "recharts": "^3.6.0",
    "resend": "^6.6.0",
    "sonner": "^2.0.7",
    "stripe": "^17.7.0",
    "swr": "^2.3.8",
    "tailwind-merge": "^3.4.0"
