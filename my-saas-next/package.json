{
  "name": "my-saas-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run stripe:listen\" \"npm run inngest:dev\" \"cd ../docs && npm run start\" --names \"NEXT,STRIPE,INNGEST,DOCS\" --prefix-colors \"blue,magenta,green,yellow\" --kill-others",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "inngest:dev": "npx inngest-cli@latest dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:seed": "npx tsx src/scripts/seed-financials.ts",
    "setup": "npm install && npm run db:push && npm run db:seed",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "setup:local": "tsx scripts/setup-local.ts",
    "docker:up": "docker-compose up -d",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "prepare": "husky",
    "generate": "npx tsx src/scripts/generate-module.ts"
  },
  "prisma": {
    "seed": "npx tsx src/scripts/seed-financials.ts"
  },
